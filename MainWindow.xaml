<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:WpfApplication1"
        Title="Example 1 (WPF)" Height="600" Width="900">
    <Window.DataContext>
        <local:Main/>
    </Window.DataContext>
    <Grid>
        <oxy:PlotView Model="{Binding Model1}"/>
        <oxy:Plot Title="{Binding Title}">
            
            <oxy:Plot.Series>
                <oxy:LineSeries ItemsSource="{Binding Lagrange}"/>
                <oxy:LineSeries ItemsSource="{Binding Newton}"/>
                <oxy:LineSeries ItemsSource="{Binding Points}" LineStyle ="None" MarkerFill ="Red" MarkerSize ="5" MarkerType ="Diamond"/>
                
            </oxy:Plot.Series>
        </oxy:Plot>
        
        
    </Grid>
</Window>
